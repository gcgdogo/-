Option Explicit
'主调用,考虑特殊拼音
Function Character_to_Pinyin(p as String) as String
	Dim pinyin as string

	pinyin = Character_to_Pinyin_Special(p)
	if pinyin="" then pinyin = Character_to_Pinyin_Ordinary(p)

	Character_to_Pinyin = pinyin
End Function

